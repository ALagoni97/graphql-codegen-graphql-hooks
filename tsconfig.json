{
    "compilerOptions": {
        "target": "ES2018",
        "module": "CommonJS",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "noImplicitOverride": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist"
    },
    "exclude": ["example"]
}
